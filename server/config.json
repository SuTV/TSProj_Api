{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "baseUrl": "http://0.0.0.0:3000",
  "webClientResetPasswordUrl": "http://127.0.0.1:8080/app/#/app/home?resetPassword=true",
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "origin": true,
      "credentials": true
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
