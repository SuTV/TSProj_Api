{
  "name": "reaction",
  "base": "PersistedModel",
  "idInjection": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "guid"
    },
    "type": {
      "type": "number",
      "required": true,
      "description": "0: Like, 1: Join, 2: Add to favorites"
    },
    "status": {
      "type": "number",
      "default": 1
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "lastUpdated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": "courseId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": []
}
