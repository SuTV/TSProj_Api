{
  "name": "streamline",
  "base": "PersistedModel",
  "idInjection": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "guid"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "type": {
      "type": "number",
      "default": 0,
      "required": true,
      "description": "0: Specific Date, 1: Daily"
    },
    "link": {
      "type": "string",
      "required": true
    },
    "provider": {
      "type": "string",
      "required": false
    },
    "embedCode": {
      "type": "string",
      "required": false
    },
    "startTime": {
      "type": "date"
    },
    "endTime": {
      "type": "date"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "lastUpdated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": "courseId"
    }
  },
  "acls": [],
  "methods": []
}
