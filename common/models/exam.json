{
  "name": "exam",
  "base": "PersistedModel",
  "idInjection": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "guid"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "startTime": {
      "type": "date",
      "required": true
    },
    "endTime": {
      "type": "date",
      "required": true
    },
    "sortNumber": {
      "type": "number",
      "default": 0,
      "required": true
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "lastUpdated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": "courseId"
    },
    "lecture": {
      "type": "belongsTo",
      "model": "lecture",
      "foreignKey": "lectureId"
    },
    "questions": {
       "type": "hasMany",
       "model": "question",
         "foreignKey": "examId"
    }
  },
  "acls": [],
  "methods": []
}
